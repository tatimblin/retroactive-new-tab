{
  "name": "Retroactive New Tab",
  "description": "Opens Link in a New Tab, after you already clicked on it.",
  "author": "https://whois.timblin.io",
  "version": "0.0.1",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "background": {
    "service_worker": "main.js",
    "type": "module"
  }
}